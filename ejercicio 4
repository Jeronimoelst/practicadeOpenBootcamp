var estacion ="verano";
switch (estacion){
case "verano":
system.out.println("es verano");
break;
case"otoño":
system.out.println("es otoño");
break;
case"invierno":
system.out.println("es invierno");
break;
case"primavera":
system.out.println("es primavera");
break;
default :
system.out.println("estoy en default");
}

